{
  "name": "velma_public",
  "version": "1.0.0",
  "description": "Public Web Documentation for EPA's Water Model VELMA 2.2",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run md-to-pug && npm run build-html",
    "build-html": "node htmlify.js",
    "build-info": "md-pug-to-html DeveloperInfo -o docs -t views",
    "build-utils": "md-pug-to-html DeveloperUtils -o docs -t views",
    "md-to-pug": "node md_to_pug.js",
    "prestart": "npm run build",
    "predev": "npm run build"
  },
  "author": "Travis Hudson",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.19.2",
    "html2pug": "^0.0.0",
    "markdown-to-pug": "^1.0.11",
    "md-pug-to-html": "^2.3.0",
    "nodemon": "^3.1.0",
    "pug": "^3.0.2"
  }
}
